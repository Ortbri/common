{
  "name": "drawlings",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "stripe:login": "bun stripe login",
    "stripe:listen": "bun stripe listen --forward-to=localhost:3000/api/webhooks",
    "stripe:fixtures": "bun stripe fixtures fixtures/stripe-fixtures.json",
    "supabase:start": "bun supabase start",
    "supabase:stop": "bun supabase stop",
    "supabase:status": "bun supabase status",
    "supabase:restart": "bun run supabase:stop && bun run supabase:start",
    "supabase:reset": "bun supabase db reset",
    "supabase:link": "bun supabase link",
    "supabase:generate-types": "bun supabase gen types typescript --local --schema public > types_db.ts",
    "supabase:generate-migration": "bun supabase db diff | bun supabase migration new",
    "supabase:generate-seed": "bun supabase db dump --data-only -f supabase/seed.sql",
    "supabase:push": "bun supabase db push",
    "supabase:pull": "bun supabase db pull",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "validate": "bun run lint && bun run format:check && bun run type-check",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "precommit": "bun run lint-staged",
    "prepush": "bun run validate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.741.0",
    "@aws-sdk/s3-request-presigner": "^3.741.0",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@stripe/stripe-js": "^5.5.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.11",
    "@upstash/redis": "^1.34.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "lucide-react": "^0.469.0",
    "motion": "^11.15.0",
    "next": "15.1.2",
    "next-safe-action": "^6.2.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^1.7.1",
    "stripe": "^17.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.1.4",
    "@next/eslint-plugin-next": "^15.1.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "supabase": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
